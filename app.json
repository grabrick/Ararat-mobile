{
  "expo": {
    "name": "ararat-mobile",
    "slug": "ararat-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "apiUri": "https://puzzle.araratchess.ru:8080/api",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "permissions": [
            "camera"
          ],
          "cameraPermission": "allow",
          "microphonePermission": "allow"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cursedimages.araratmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.cursedimages.araratmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e43bfbe3-5f95-4d72-8586-505a1eb5616a"
      }
    }
  }
}
